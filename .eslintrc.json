{
  "extends": "eslint:recommended",
  "plugins": [ "react" ],
  "rules": {
    "max-len": [1, 120, 2, {"ignoreComments": true}],
    "no-console": 0,
    "no-unused-vars": [0, {"vars": "local", "args": "none"}],
    "no-multiple-empty-lines": 0,
    "react/jsx-uses-react": 1,
    "semi": [0, "always"]
  },
  "globals": {
    "React": true,
    "module": true
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  }
}
